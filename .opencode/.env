# opencode Token and Cost Configuration for Kimi
OPENCODE_SHOW_TOKENS=true
OPENCODE_SHOW_COSTS=true
OPENCODE_TRACK_USAGE=true
OPENCODE_MODEL=kimi-latest

# Moonshot AI (Kimi) API Key
MOONSHOT_API_KEY=your_api_key_here

# Kimi model settings
OPENCODE_PROVIDER=moonshot
OPENCODE_MODEL=moonshotai/kimi-k2-0711-preview

# Cost tracking
OPENCODE_CURRENCY=USD
OPENCODE_LOG_LEVEL=info